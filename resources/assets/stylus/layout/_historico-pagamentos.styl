#header-painel
  position relative
  display block
  width 100%
  padding 20px 0px 0 0
  background #fff

  h1.logo
    margin 0px
    width 100%
    max-width 222px
    background url("../img/logo-header.png") no-repeat top left
    background-size contain
    height 50px
    float none
    margin 0 auto

    a
      display block
      width 100%
      text-indent -9999px

  #menu
    column(9/9)
    background #f4f4f4
    margin-top 20px
    padding 20px 0;
    float none

    ul
      position relative
      display table
      list-style none
      padding 0px
      margin  0px auto 0px auto;

      li
        position relative
        display block
        float left
        padding-right 30px
        line-height 11px
        font-size 13px

        a
          color blue
          font-size 13px
          font-weight 700
          text-transform uppercase
          text-decoration none

          &:hover
            color: #0071fd;

      .free-test
        padding 0px

        a
          color #fff
          text-align center
          border-radius 5px
          background-color orange
          padding 5px 41px
          transition all 0.5s ease

          &:hover
            background-color lighten(orange, 10)

.main-painel.history-pay
  position relative
  display block
  width 100%
  padding 40px 0 140px 0
  background-color #185fb7
  margin-bottom -80px

  .container
    position relative
    display block

  .bg-bl
    position absolute
    display block
    top -20px
    left 0px
    z-index 10

  h2
    margin: 0px
    padding: 0px
    text-align: center
    color: #fff
    font-size: 24px
    line-height 23px
    text-transform uppercase

  .table-base
    position relative
    display block
    width 100%
    max-width 770px
    margin 0 auto
    box-sizing border-box
    background #fff
    border-radius 5px
    padding 70px 80px 35px 80px
    margin-top 39px
    z-index 100

    .btn-orange
      width 100%
      max-width 230px
      margin 30px auto 15px auto

    .cancel-subs
      position relative
      display block
      width 100%
      text-align center
      color #666666
      font-size 14px
      text-decoration none
      font-weight 700

  .table.table-responsive
    box-sizing border-box
    width: 100%;
    max-width: 100%;

    thead,
    tbody
      width 100%

    thead tr th
      font-size 16px
      text-transform uppercase
      padding-bottom 20px

    tbody tr td
      font-size 14px
      padding-bottom 15px
      padding-top 15px

      .btn-green,
      .btn-gray
        display block
        width 100%
        padding 8px 0

      .btn-gray
        display block
        width 100%
        padding 8px 0
        color #fff
        font-size 14px
        text-transform uppercase
        text-align center
        font-weight 700
        width auto
        min-width 160px
        text-decoration none
        border-radius 5px
        background #b6b6b6

    tbody tr
      border-bottom 2px solid #e3e3e3

    th,
    th
      text-align left
